{
    "env": {
        "es2021": true,
        "node": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "indent": [ "error", 2 ],
        "no-console": "off",
        "no-underscore-dangle": "off"
    }
}
